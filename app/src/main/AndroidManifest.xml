<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_learning.e_learningapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomePage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SubjectList"
            android:label="Subjects" />
        <activity
            android:name=".TopicList"
            android:label="Topics" />
        <activity
            android:name=".Buttons"
            android:label="Activity"></activity>
        <activity
            android:name=".SuppList"
            android:label="Notebook"></activity>
        <activity
            android:name=".VideoList"
            android:label="Videos"></activity>
    </application>

</manifest>